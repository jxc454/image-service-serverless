{
  "name": "image-service-serverless",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start": "sls offline start --noAuth --stage local",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "./node_modules/.bin/prettier --write '**/*.ts'",
    "lint": "eslint --fix --max-warnings=-1 '**/*.ts'",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-header-normalizer": "^2.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "@middy/http-multipart-body-parser": "^2.5.2",
    "aws-sdk": "^2.1006.0",
    "config": "^3.3.6",
    "dynamoose": "^2.8.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/config": "0.0.39",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.0",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "serverless": "^2.23.0",
    "serverless-dynamodb-client": "0.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.17.1",
    "serverless-offline": "^8.2.0",
    "serverless-offline-dynamodb-streams": "^5.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.4"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "lint-staged": {
    "./**/*.ts": [
      "eslint --fix --max-warnings=-1",
      "./node_modules/.bin/prettier --write"
    ]
  }
}
